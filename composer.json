{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.12.1",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "graham-campbell/markdown": "^13.1",
        "grimzy/laravel-mysql-spatial": "^5.0",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/nexmo-notification-channel": "^2.3",
        "laravel/passport": "^10.0",
        "laravel/scout": "^9.4",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mews/purifier": "^3.3",
        "mmedia/laravel-collaborative-filtering": "dev-master",
        "pusher/pusher-php-server": "~4.0",
        "spatie/laravel-cookie-consent": "^2.12.0",
        "spatie/laravel-honeypot": "^3.0",
        "spatie/laravel-image-optimizer": "^1.3",
        "spatie/laravel-permission": "^5.0",
        "valorin/pwned-validator": "^1.2",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html test-results/coverage",
        "test-dox": "vendor/bin/phpunit --testdox-text test-results/testDox",
        "lint": "vendor/bin/phpcs --standard=PSR12 --exclude=Generic.Files.LineLength app",
        "lint-fix": "vendor/bin/phpcbf --standard=PSR12 app",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}